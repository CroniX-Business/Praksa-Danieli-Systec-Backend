{
  "ConnectionStrings": {
    "DefaultConnection" : "Server=.\\SQLExpress;Database=OrderDb;Trusted_Connection=true;TrustServerCertificate=true;"
  },

  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.File" ],
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "File",
        "Args": {
          "path": "logs/myLogs-.txt",
          "rollingInterval": "Day"
        }
      }
    ]
  },
  "AllowedHosts": "*"
}
